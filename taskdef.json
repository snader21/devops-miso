{
  "executionRoleArn": "arn:aws:iam::905418334417:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "portMappings": [
        {
          "hostPort": 5000,
          "protocol": "tcp",
          "containerPort": 5000
        }
      ],

      "image": "<IMAGE1_NAME>",
      "essential": true,
      "environment": [
        {
          "name": "DB_NAME",
          "value": "blacklist"
        },
        {
          "name": "API_KEY",
          "value": "my-secret-key"
        },
        {
          "name": "DB_PORT",
          "value": "5432"
        },
        {
          "name": "DB_HOST",
          "value": "database-fargate.cbwmmisewnbr.us-east-1.rds.amazonaws.com"
        },
        {
          "name": "DB_USER",
          "value": "postgres"
        },
        {
          "name": "DB_PASSWORD",
          "value": "postgres"
        }
      ],
      "name": "python-app"
    }
  ],

  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "512",
  "memory": "1024",
  "family": "Task--app-python"
}
